{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "freeswitch",
      "image": "dwilkie/freeswitch-rayo",
      "essential": true,
      "memory": 950,
      "portMappings": [
        {
          "hostPort": 5222,
          "containerPort": 5222,
          "protocol": "tcp"
        },
        {
          "hostPort": 5060,
          "containerPort": 5060,
          "protocol": "udp"
        }
      ],
      "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
              "awslogs-group": "awslogs-freeswitch",
              "awslogs-region": "ap-southeast-1",
              "awslogs-stream-prefix": "awslogs-freeswitch"
          }
      },
      "environment": [
        {
          "name": "SECRETS_BUCKET_NAME",
          "value": "somlengsecrets"
        },
        {
          "name": "FREESWITCH_CONF_DIR",
          "value": "freeswitch/conf"
        }
      ]
    }
  ]
}
