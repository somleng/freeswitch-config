{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "freeswitch",
      "image": "somleng/somleng-freeswitch",
      "essential": true,
      "memoryReservation": 8192,
      "cpu": 16384,
      "portMappings": [
        {
          "hostPort": 5222,
          "containerPort": 5222,
          "protocol": "tcp"
        },
        {
          "hostPort": 5060,
          "containerPort": 5060,
          "protocol": "udp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": true,
          "awslogs-group": "/aws/elasticbeanstalk/somleng-freeswitch-webserver/freeswitch.log",
          "awslogs-region": "ap-southeast-1"
        }
      }
    }
  ]
}
